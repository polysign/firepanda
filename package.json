{
  "name": "firepanda",
  "version": "0.0.8",
  "description": "A Firestore ORM built using Typescript",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "cli:firepanda": "node bin/firepanda",
    "cli": "npm-run-all -l cli:*",
    "test:jest": "jest",
    "test:jest:ci": "jest --ci --runInBand --detectOpenHandles",
    "test:jest:watch": "jest --watch",
    "test:firestore:emulator": "firebase emulators:start --only firestore",
    "ci": "start-server-and-test test:firestore:emulator http-get://127.0.0.1:8080 test:jest:ci"
  },
  "author": "Georges Jentgen <georges@polysign.lu>",
  "license": "ISC",
  "bin": {
    "firepanda": "./cli.js"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "firebase": "^6.2.2",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@firebase/testing": "^0.11.2",
    "delay": "^4.3.0",
    "enquirer": "^2.3.0",
    "execa": "^1.0.0",
    "firebase-admin": "^8.2.0",
    "firebase-functions": "^3.0.1",
    "fs-extra": "^8.0.1",
    "meow": "^5.0.0",
    "mkdirp": "^0.5.1",
    "mkdirp-promise": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "ora": "^3.4.0",
    "start-server-and-test": "^1.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git@polysign.github.com:polysign/firepanda.git"
  },
  "keywords": [
    "firebase",
    "firestore",
    "google-cloud",
    "cloud functions",
    "serverless",
    "typescript",
    "framework"
  ]
}
